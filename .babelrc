{
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "*": "./src/"
        }
      }
    ],
    [
      "react-hot-loader/babel"
    ],
    [
      "@babel/plugin-proposal-class-properties"
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ]
}